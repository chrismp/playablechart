// Mixins
@import 'compass';
@import 'compass/reset';


@import "compass/css3/transition";
@import 'bootstrap';


 $brand-chart-secondary: #f6901e;

 $transparent : rgba(0,0,0,00);
 
 path.line {
  fill: none;
  stroke: $brand-chart-secondary;
  stroke-width: 1.5px;
}

path.area {
  fill: #e7e7e7;
}

.axis {
  shape-rendering: crispEdges;
}

.x.axis line {
  stroke: #fff;
}

.x.axis .minor {
  stroke-opacity: .5;
}

.x.axis path {
  display: none;
}

.y.axis line, .y.axis path {
  fill: none;
  stroke: #666;
}